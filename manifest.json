{
  "name": "Translator",
  "description": "Fetches dank translations",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
      }
  ],
  "permissions": [
      "tabs",
      "storage"
  ],
  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/16-uganda.png"
  }
}